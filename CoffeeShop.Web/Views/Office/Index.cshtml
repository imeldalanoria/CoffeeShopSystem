@model IEnumerable<CoffeeShop.Model.OfficeModel>

@{
    ViewBag.Title = "Offices";
}

<h2>Offices</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OfficeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PantryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OfficeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PantryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OfficeID }) 
        </td>
    </tr>
}

</table>
